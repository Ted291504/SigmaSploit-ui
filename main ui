-- Scripts:

local function GWZS_script() -- Execute.LocalScript 
	local script = Instance.new('LocalScript', Execute)

	local button = script.Parent
	local textbox = script.Parent.Parent.Parent.TextBox
	
	button.MouseButton1Click:Connect(function()
		loadstring(textbox.Text)()
	end)
end
coroutine.wrap(GWZS_script)()
local function QJHPYMT_script() -- Clear.LocalScript 
	local script = Instance.new('LocalScript', Clear)

	local button = script.Parent
	local textbox = script.Parent.Parent.Parent.TextBox
	
	button.MouseButton1Click:Connect(function()
		textbox.Text = ""
	end)
end
coroutine.wrap(QJHPYMT_script)()
local function YMWWMS_script() -- Copy.LocalScript 
	local script = Instance.new('LocalScript', Copy)

	local button = script.Parent
	local textbox = script.Parent.Parent.Parent.TextBox
	
	button.MouseButton1Click:Connect(function()
		setclipboard(textbox.Text)
	end)
end
coroutine.wrap(YMWWMS_script)()
local function MWJQLS_script() -- Paste.LocalScript 
	local script = Instance.new('LocalScript', Paste)

	local button = script.Parent
	local textbox = script.Parent.Parent.Parent.TextBox
	
	button.MouseButton1Click:Connect(function()
		textbox.Text = getclipboard()
	end)
end
coroutine.wrap(MWJQLS_script)()
local function ERLDZV_script() -- FPSTitle.LocalScript 
	local script = Instance.new('LocalScript', FPSTitle)

	local textLabel = script.Parent
	local runService = game:GetService("RunService")
	
	local lastTime = tick()
	local fps = 0
	
	runService.RenderStepped:Connect(function()
		local currentTime = tick()
		fps = math.floor(1 / (currentTime - lastTime))
		lastTime = currentTime
	
		textLabel.Text = "Current FPS : " .. fps
	end)
	
end
coroutine.wrap(ERLDZV_script)()
local function LPAR_script() -- InfYieldBTN.LocalScript 
	local script = Instance.new('LocalScript', InfYieldBTN)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/edgeiy/infiniteyield/master/source"))()
	end)
end
coroutine.wrap(LPAR_script)()
