TextBox.Size = UDim2.new(0, 368, 0, 182)
TextBox.ClearTextOnFocus = false
TextBox.Font = Enum.Font.SourceSans
TextBox.MultiLine = true
TextBox.PlaceholderText = "-- [[\nHybrid Android 2.0.0\nCreated by offset_fucker\n]] --"
TextBox.Text = isfile('HybridContents.lua') and readfile('HybridContents.lua') or ""
TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
TextBox.TextSize = 12.000
TextBox.TextWrapped = true
TextBox.TextXAlignment = Enum.TextXAlignment.Left
TextBox.TextYAlignment = Enum.TextYAlignment.Top
TextBox:GetPropertyChangedSignal('Text'):Connect(function()
    writefile('HybridContents.lua', tostring(TextBox.Text))
    local tab = {}
    for i = 1, (#TextBox.Text:split('\n')) do
        table.insert(tab, tostring(i))
    end
end)

