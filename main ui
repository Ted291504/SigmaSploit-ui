-- Scripts:

local function GEIPR_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	local textBox = script.Parent
	local scrollingFrame = script.Parent.Parent.ScrollingFrame
	local httpService = game:GetService("HttpService")
	
	if not scrollingFrame:FindFirstChild("UIListLayout") then
		local listLayout = Instance.new("UIListLayout")
		listLayout.Parent = scrollingFrame
		listLayout.SortOrder = Enum.SortOrder.LayoutOrder
		listLayout.Padding = UDim.new(0, 5)
	end
	
	local function clearResults()
		for _, child in ipairs(scrollingFrame:GetChildren()) do
			if child:IsA("Frame") then
				child:Destroy()
			end
		end
		scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
	end
	
	local function createRoundedFrame(parent, size, position)
		local frame = Instance.new("Frame")
		frame.Size = size
		frame.Position = position
		frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		frame.BorderSizePixel = 0
		frame.Parent = parent
	
		local corner = Instance.new("UICorner")
		corner.CornerRadius = UDim.new(0, 8)
		corner.Parent = frame
	
		return frame
	end
	
	local function searchScripts(query)
		clearResults()
	
		local url = "https://scriptblox.com/api/script/search?q=" .. query
		local success, response = pcall(function()
			return game:HttpGet(url)
		end)
	
		if success then
			local data = httpService:JSONDecode(response)
	
			if data and data.result and data.result.scripts then
				for _, scriptData in ipairs(data.result.scripts) do
					local scriptName = scriptData.title or "Unknown"
					local scriptContent = scriptData.script or ""
					local isVerified = scriptData.verified or false
					local hasKey = scriptData.key or false
					local gameName = scriptData.game and scriptData.game.name or "Unknown Game"
					local views = scriptData.views or 0
	
					-- Monstrum's Gui to Lua\n-- Version: 3.2
	
					-- Instances:
	
					local Results = Instance.new("Frame")
					local UIStroke = Instance.new("UIStroke")
					local UICorner = Instance.new("UICorner")
					local Game = Instance.new("TextLabel")
					local ExecuteFrame = Instance.new("Frame")
					local UICorner_2 = Instance.new("UICorner")
					local UIStroke_2 = Instance.new("UIStroke")
					local Execute = Instance.new("ImageButton")
					local Script = Instance.new("TextLabel")
					local Verified = Instance.new("TextLabel")
					local Key = Instance.new("TextLabel")
	
					--Properties:
	
					Results.Name = "Results"
					Results.Parent = script.Parent.Parent.ScrollingFrame
					Results.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
					Results.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Results.BorderSizePixel = 0
					Results.Position = UDim2.new(0.0344734304, 0, 0.0200803839, 0)
					Results.Size = UDim2.new(0, 337, 0, 51)
	
					UIStroke.Parent = Results
					UIStroke.Color = Color3.fromRGB(86, 86, 86)
					UIStroke.Thickness = 2.000
	
					UICorner.Parent = Results
	
					Game.Name = "Game"
					Game.Parent = Results
					Game.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					Game.BackgroundTransparency = 1.000
					Game.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Game.BorderSizePixel = 0
					Game.Position = UDim2.new(0.326409489, 0, 0.0196078438, 0)
					Game.Size = UDim2.new(0, 117, 0, 21)
					Game.Font = Enum.Font.Ubuntu
					Game.TextColor3 = Color3.fromRGB(255, 255, 255)
					Game.TextSize = 14.000
					Game.TextWrapped = true
					Game.Text = "Game: " .. gameName
	
					ExecuteFrame.Name = "ExecuteFrame"
					ExecuteFrame.Parent = Results
					ExecuteFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
					ExecuteFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
					ExecuteFrame.BorderSizePixel = 0
					ExecuteFrame.Position = UDim2.new(0.873442233, 0, 0.155626789, 0)
					ExecuteFrame.Size = UDim2.new(0, 34, 0, 34)
	
					UICorner_2.Parent = ExecuteFrame
	
					UIStroke_2.Parent = ExecuteFrame
					UIStroke_2.Color = Color3.fromRGB(86, 86, 86)
					UIStroke_2.Thickness = 2.000
	
					Execute.Name = "Execute"
					Execute.Parent = ExecuteFrame
					Execute.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					Execute.BackgroundTransparency = 1.000
					Execute.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Execute.BorderSizePixel = 0
					Execute.Position = UDim2.new(0.0788825527, 0, 0.126588255, 0)
					Execute.Size = UDim2.new(0, 28, 0, 26)
					Execute.Image = "rbxassetid://111121478238230"
					Execute.MouseButton1Click:Connect(function()
						local success, err = pcall(function()
							loadstring(scriptContent)()
						end)
						if not success then
							warn("Error executing script:", err)
						end
					end)
	
					Script.Name = "Script"
					Script.Parent = Results
					Script.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					Script.BackgroundTransparency = 1.000
					Script.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Script.BorderSizePixel = 0
					Script.Position = UDim2.new(0.326409489, 0, 0.431372553, 0)
					Script.Size = UDim2.new(0, 117, 0, 21)
					Script.Font = Enum.Font.Ubuntu
					Script.Text = scriptName
					Script.TextColor3 = Color3.fromRGB(255, 255, 255)
					Script.TextSize = 12.000
					Script.TextWrapped = true
	
					Verified.Name = "Verified"
					Verified.Parent = Results
					Verified.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					Verified.BackgroundTransparency = 1.000
					Verified.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Verified.BorderSizePixel = 0
					Verified.Position = UDim2.new(0.0207715128, 0, 0.117647059, 0)
					Verified.Size = UDim2.new(0, 52, 0, 21)
					Verified.Font = Enum.Font.Ubuntu
					Verified.Text = isVerified and "âœ…" or "âŒ"
					Verified.TextColor3 = Color3.fromRGB(255, 255, 255)
					Verified.TextSize = 12.000
					Verified.TextWrapped = true
	
					Key.Name = "Key"
					Key.Parent = Results
					Key.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					Key.BackgroundTransparency = 1.000
					Key.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Key.BorderSizePixel = 0
					Key.Position = UDim2.new(0.0207715128, 0, 0.431372553, 0)
					Key.Size = UDim2.new(0, 52, 0, 21)
					Key.Font = Enum.Font.Ubuntu
					Key.TextColor3 = Color3.fromRGB(255, 255, 255)
					Key.TextSize = 12.000
					Key.TextWrapped = true
					Key.Text = "Key: " .. (hasKey and "ðŸ”‘" or "âŒ")
					-- Scripts:
	
					local function STASN_script() -- Execute.LocalScript 
						local script = Instance.new('LocalScript', Execute)
	
						local button = script.Parent
	
						button.MouseButton1Click:Connect(function()
							loadstring(scriptContent)()
						end)
					end
					coroutine.wrap(STASN_script)()
				end
	
				task.wait()
				scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, scrollingFrame.UIListLayout.AbsoluteContentSize.Y)
			end
		end
	end
	
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
		if #textBox.Text > 0 then
			searchScripts(textBox.Text)
		else
			clearResults()
		end
	end)
end
coroutine.wrap(GEIPR_script)()
