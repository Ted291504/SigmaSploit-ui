local function NLNA_fake_script() -- Execute_button.LocalScript 
	local script = Instance.new('LocalScript', Execute_button)

	local TextBox = script.Parent.Parent.Exec_text
	
	local ExecuteButton = script.Parent
	ExecuteButton.MouseButton1Click:Connect(function()
		local luaCode = TextBox.Text
		local success, err = pcall(function()
			loadstring(luaCode)()
		end)
	
		if not success then
			warn("Script failed to execute error: .. err")
		end
	end)
	
end
coroutine.wrap(NLNA_fake_script)()
local function TYIKKE_fake_script() -- Clear_button.LocalScript 
	local script = Instance.new('LocalScript', Clear_button)

	local TextBox = script.Parent.Parent.Exec_text
	local ClearButton = script.Parent
	ClearButton.MouseButton1Click:Connect(function()
		TextBox.Text = ""
	end)
	
end
coroutine.wrap(TYIKKE_fake_script)()
local function RVYZQI_fake_script() -- Paste_button.LocalScript 
	local script = Instance.new('LocalScript', Paste_button)

	TextBox = script.Parent.Parent.Exec_text
	script.Parent.MouseButton1Click:Connect(function()
		TextBox.Text = getclipboard()
	end)
end
coroutine.wrap(RVYZQI_fake_script)()
local function LCATB_fake_script() -- Search.LocalScript 
	local script = Instance.new('LocalScript', Search)

	local textBox = script.Parent
	local scrollingFrame = script.Parent.Parent.ScrollingFrame
	local httpService = game:GetService("HttpService")
	
	if not scrollingFrame:FindFirstChild("UIListLayout") then
		local listLayout = Instance.new("UIListLayout")
		listLayout.Parent = scrollingFrame
		listLayout.SortOrder = Enum.SortOrder.LayoutOrder
		listLayout.Padding = UDim.new(0, 5)
	end
	
	local function clearResults()
		for _, child in ipairs(scrollingFrame:GetChildren()) do
			if child:IsA("Frame") then
				child:Destroy()
			end
		end
		scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
	end
	
	local function createRoundedFrame(parent, size, position)
		local frame = Instance.new("Frame")
		frame.Size = size
		frame.Position = position
		frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		frame.BorderSizePixel = 0
		frame.Parent = parent
	
		local corner = Instance.new("UICorner")
		corner.CornerRadius = UDim.new(0, 8)
		corner.Parent = frame
	
		return frame
	end
	
	local function searchScripts(query)
		clearResults()
	
		local url = "https://scriptblox.com/api/script/search?q=" .. query
		local success, response = pcall(function()
			return game:HttpGet(url)
		end)
	
		if success then
			local data = httpService:JSONDecode(response)
	
			if data and data.result and data.result.scripts then
				for _, scriptData in ipairs(data.result.scripts) do
					local scriptName = scriptData.title or "Unknown"
					local scriptContent = scriptData.script or ""
					local isVerified = scriptData.verified or false
					local hasKey = scriptData.key or false
					local gameName = scriptData.game and scriptData.game.name or "Unknown Game"
					local views = scriptData.views or 0
	
					local resultFrame = createRoundedFrame(scrollingFrame, UDim2.new(1, -10, 0, 80), UDim2.new(0, 5, 0, 0))
					resultFrame.LayoutOrder = #scrollingFrame:GetChildren()
	
					local verifiedLabel = Instance.new("TextLabel")
					verifiedLabel.Size = UDim2.new(0, 20, 0, 20)
					verifiedLabel.Position = UDim2.new(0, 5, 0, 5)
					verifiedLabel.Text = isVerified and "âœ…" or "âŒ"
					verifiedLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
					verifiedLabel.BackgroundTransparency = 1
					verifiedLabel.Parent = resultFrame
	
					local viewsLabel = Instance.new("TextLabel")
					viewsLabel.Size = UDim2.new(0, 60, 0, 20)
					viewsLabel.Position = UDim2.new(1, -65, 0, 5)
					viewsLabel.Text = "ðŸ‘€ " .. views
					viewsLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
					viewsLabel.TextXAlignment = Enum.TextXAlignment.Right
					viewsLabel.BackgroundTransparency = 1
					viewsLabel.Parent = resultFrame
	
					local detailsFrame = Instance.new("Frame")
					detailsFrame.Size = UDim2.new(0.7, -10, 1, -10)
					detailsFrame.Position = UDim2.new(0, 30, 0.5, -30)
					detailsFrame.BackgroundTransparency = 1
					detailsFrame.Parent = resultFrame
	
					local scriptNameLabel = Instance.new("TextLabel")
					scriptNameLabel.Size = UDim2.new(1, 0, 0.4, 0)
					scriptNameLabel.Text = scriptName
					scriptNameLabel.TextScaled = true
					scriptNameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
					scriptNameLabel.BackgroundTransparency = 1
					scriptNameLabel.Parent = detailsFrame
	
					local gameNameLabel = Instance.new("TextLabel")
					gameNameLabel.Size = UDim2.new(1, 0, 0.3, 0)
					gameNameLabel.Position = UDim2.new(0, 0, 0.4, 0)
					gameNameLabel.Text = "Game: " .. gameName
					gameNameLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
					gameNameLabel.BackgroundTransparency = 1
					gameNameLabel.Parent = detailsFrame
	
					local keyLabel = Instance.new("TextLabel")
					keyLabel.Size = UDim2.new(1, 0, 0.3, 0)
					keyLabel.Position = UDim2.new(0, 0, 0.7, 0)
					keyLabel.Text = "Key: " .. (hasKey and "ðŸ”‘" or "âŒ")
					keyLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
					keyLabel.BackgroundTransparency = 1
					keyLabel.Parent = detailsFrame
	
					local executeButton = Instance.new("TextButton")
					executeButton.Size = UDim2.new(0.1, 0, 0.5, 0)
					executeButton.Position = UDim2.new(0.88, 0, 0.25, 0)
					executeButton.BackgroundColor3 = Color3.fromRGB(0, 100, 255)
					executeButton.Text = "Execute"
					executeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
					executeButton.Parent = resultFrame
	
					local buttonCorner = Instance.new("UICorner")
					buttonCorner.CornerRadius = UDim.new(0, 8)
					buttonCorner.Parent = executeButton
	
					executeButton.MouseButton1Click:Connect(function()
						local success, err = pcall(function()
							loadstring(scriptContent)()
						end)
						if not success then
							warn("Error executing script:", err)
						end
					end)
				end
	
				task.wait()
				scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, scrollingFrame.UIListLayout.AbsoluteContentSize.Y)
			end
		end
	end
	
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
		if #textBox.Text > 0 then
			searchScripts(textBox.Text)
		else
			clearResults()
		end
	end)
end
coroutine.wrap(LCATB_fake_script)()
local function LACGEO_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	local UIS = game:GetService("UserInputService")
	local button = script.Parent
	local dragging, dragInput, dragStart, startPos
	
	button.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = button.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	button.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - dragStart
			button.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
	local UserInputService = game:GetService("UserInputService")
	
	
	local MainFrame = script.Parent
	local TopBar = script.Parent
	
	local Camera = workspace:WaitForChild("Camera")
	
	local DragMousePosition
	local FramePosition
	
	
	local Draggable = false
	
	TopBar.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			Draggable = true
			DragMousePosition = Vector2.new(input.Position.X, input.Position.Y)
			FramePosition= Vector2.new(MainFrame.Position.X.Scale, MainFrame.Position.Y.Scale)
		end
	end)
	
	TopBar.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			Draggable = false
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if Draggable == true then
			local NewPosition = FramePosition + ( (Vector2.new(input.Position.X, input.Position.Y) - DragMousePosition) / Camera.ViewportSize)
			MainFrame.Position = UDim2.new(NewPosition.X, 0, NewPosition.Y, 0)
		end
	end)
end
coroutine.wrap(LACGEO_fake_script)()
local function GJGWWY_fake_script() -- Show_button.LocalScript 
	local script = Instance.new('LocalScript', Show_button)

	local UIS = game:GetService("UserInputService")
	local button = script.Parent
	local dragging, dragInput, dragStart, startPos
	local function AlreadyVisible()
		if button.Parent.Main.Visible == true then
			button.Text = "Show"
			button.Parent.Main.Visible = false
		else
			button.Parent.Main.Visible = true
			button.Text = "Hide"
		end
	end
	button.MouseButton1Click:Connect(function()
		AlreadyVisible()
	end)
	
	button.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = button.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	button.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - dragStart
			button.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
	
end
coroutine.wrap(GJGWWY_fake_script)()
