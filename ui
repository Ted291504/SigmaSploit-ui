local function ZJTA_fake_script() -- Home_Button.LocalScript 
	local script = Instance.new('LocalScript', Home_Button)

	local TweenService = game:GetService("TweenService")
	
	local Tab = script.Parent.Parent.Parent.Home_Page
	
	local OpenTab = script.Parent
	
	local function TabOpen()
		script.Parent.Parent.Parent.Script_Search_Page.Visible = false
		script.Parent.Parent.Parent.Exec_Page.Visible = false
		Tab.Visible = true
		Tab.Transparency = 1
		Tab.Transparency = 0.9
		wait(0.1)
		Tab.Transparency = 0.8
		wait(0.1)
		Tab.Transparency = 0.7
		wait(0.1)
		Tab.Transparency = 0.6
		wait(0.1)
		Tab.Transparency = 0.5
		wait(0.1)
		Tab.Transparency = 0.4
		wait(0.1)
		Tab.Transparency = 0.3
		wait(0.1)
		Tab.Transparency = 0.2
		wait(0.1)
		Tab.Transparency = 0.1
		wait(0.1)
		Tab.Transparency = 0
	end
	
	
	local function OpenedTab()
		-- game:GetService("StarterGui"):SetCore("SendNotification",{
		-- Title = "Tab Already Opened",
		-- Text = "Diet Coke is not gay!",
		-- })
	end
	
	OpenTab.MouseButton1Click:Connect(function()
		if Tab.Visible == true then
			OpenedTab()
		else
			TabOpen()
		end
	end)
end
coroutine.wrap(ZJTA_fake_script)()
